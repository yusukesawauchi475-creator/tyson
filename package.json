{
  "name": "tyson",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "node server.js",
    "dev:all": "concurrently \"npm run dev:server\" \"npm run dev\"",
    "build": "rm -rf node_modules/.vite && rm -rf dist && vite build && (grep -rq '5秒' dist 2>/dev/null && (echo 'BUILD ABORT: 5秒 found in dist' && exit 1) || exit 0)",
    "build:vercel": "VITE_BUILD_SHA=$VERCEL_GIT_COMMIT_SHA npm run build",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy-and-prove": "bash scripts/deploy-and-prove.sh",
    "cors:set": "bash setup-storage-cors.sh",
    "cors:prove": "node scripts/set-cors-and-prove.js",
    "verify:parse": "node scripts/verify-parse-ceo.js",
    "verify:retry": "node scripts/verify-retry-ceo.js",
    "verify:keys": "node scripts/verify-ceo-keys.js",
    "verify:reset": "node scripts/verify-reset-bulletproof.js",
    "verify:analyze": "node scripts/verify-analyze-ceo.js",
    "verify:deploy": "node scripts/verify-deploy-health.js",
    "verify:streak": "node scripts/verify-streak-theme.js",
    "verify:admin": "node scripts/verify-admin.js",
    "verify:audio": "node scripts/verify-audio-bulletproof.js",
    "verify:date": "node scripts/verify-date-display.js",
    "verify": "node scripts/verify_all.mjs",
    "soak:verify": "node scripts/soak_verify.mjs",
    "kill-orange": "node scripts/kill-orange.js",
    "smoke:uiCopy": "node scripts/smoke_uiCopy.mjs"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "firebase": "^12.8.0",
    "firebase-admin": "^13.6.0",
    "form-data": "^4.0.5",
    "openai": "^6.16.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.13.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@google-cloud/storage": "^7.14.0",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "vite": "^7.2.4"
  },
  "overrides": {
    "shell-quote": "^1.8.1"
  }
}
